{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension" : "off",
      "react/prop-types": "off",
      "react/jsx-props-no-spreading": "off",
      "import/order": "off",
      "no-undef": "off",
      "no-console": "off",
      "arrow-body-style": "off",
      "react/self-closing-comp": "off",
      "import/no-unresolved": "off"
    },
    "env": {
      "jest": true
    }


}
